{
  "env": {
    "node": true,
    "jest": true
  },
  "extends": ["eslint:recommended", "plugin:security/recommended"],
  "plugins": ["no-secrets"],
  "parserOptions": {
    "ecmaVersion": 2022
  },
  "rules": {
    "no-restricted-imports": ["error", {"patterns": ["^/"]}],
    "no-secrets/no-secrets": "error",
    "require-jsdoc": "error"
  }
}
